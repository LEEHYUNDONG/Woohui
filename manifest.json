{
    "name": "Woohui",
    "description": "Search the keyword on the other pages",

    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "host_permissions" : ["http://*/*", "https://*/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/extension.png",
            "32": "/images/extension.png",
            "48": "/images/extension.png",
            "128": "/images/extension.png"
          }
      },
      "icons": {
        "16": "/images/extension.png",
        "32": "/images/extension.png",
        "48": "/images/extension.png",
        "128": "/images/extension.png"
      }
}